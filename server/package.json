  "scripts": {
    "start": "node dist/src/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "migrate": "prisma migrate dev",
    "generate": "prisma generate",
    "seed": "ts-node prisma/seed.ts",
    "remigrate": "prisma migrate reset --force && npm run migrate",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:db:reset": "cross-env NODE_ENV=test DATABASE_URL=postgresql://test_user:test_pass@localhost:5433/testlabchatdb npx prisma migrate deploy",
    "test:db:seed": "cross-env NODE_ENV=production DATABASE_URL=postgresql://test_user:test_pass@localhost:5433/testlabchatdb npx ts-node src/seed.ts",
    "test:users": "jest tests/discussion/post.test.ts"
  },
